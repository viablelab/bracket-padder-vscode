{
  "name": "bracket-padder",
  "displayName": "bracket-padder",
  "description": "Smart whitespace padding & auto-closing for bracket pairs",
  "version": "0.3.0",
  "publisher": "viablelab",
  "repository": "https://github.com/viablelab/bracket-padder-vscode",
  "engines": {
    "vscode": "^1.40.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "configuration": {
      "type": "object",
      "title": "bracket-padder",
      "properties": {
        "bracket-padder.smartCloseMaxParseLength": {
          "type": [
            "number"
          ],
          "default": 80,
          "description": "How many previous characters on a line should be parsed to determine whether to auto close bracket"
        },
        "bracket-padder.smartPairs": {
          "type": [
            "object"
          ],
          "default": {
            "{": "}",
            "[": "]",
            "(": ")"
          },
          "description": "Pairs handled by this extension"
        }
      }
    }
  },
  "scripts": {
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile",
    "test": "node ./out/test/runTest.js",
    "vsce-package": "vsce package"
  },
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.12.12",
    "@types/ramda": "^0.26.36",
    "@types/vscode": "^1.40.0",
    "glob": "^7.1.6",
    "mocha": "^6.2.2",
    "ramda": "^0.26.1",
    "typescript": "^3.7.2",
    "vsce": "^1.69.0",
    "vscode-test": "^1.2.3"
  }
}
